$("div[id*=<%= @cupping_crust_aroma.sample.id %>]").find("form[id*=crust_aroma]").before('<div id="crust-aroma-show"><b>Crust Aroma:</b> Score: <%= @cupping_crust_aroma.score %><% if @cupping_crust_aroma.intensity.present? %> Intensity: <%= @cupping_crust_aroma.intensity %><% end %><% if @cupping_crust_aroma.notes.present? %> Notes: <%= @cupping_crust_aroma.notes %><% end %> <%= link_to 'Re-do', @cupping_crust_aroma, :method => :delete, :remote => true %></div>');

$("div[id*=<%= @cupping_crust_aroma.sample.id %>]").find("form[id*=crust_aroma]").hide();

$("div[id*=<%= @cupping_crust_aroma.sample.id %>]").find("div[id=total]").html("<b>Total Score:</b> <%= @cupping_total_score.score %>");

