<section class="sectionlist">
	<h2>Producer Admins</h2>
	<section>
		<% @producer_admins.each do |producer_admin| %>
			<p>
				<%= link_to 'Remove', producer_admin, :confirm => 'Are you sure?', :method => :delete, :class => "removebutton" %>
				<b><%= producer_admin.user.email %></b> for <b><%= link_to producer_admin.farm.name, farm_path(:id => producer_admin.farm.id) %></b>
			</p>
		<% end %>
	</section>
</section>