<section id="acidity-show">
	<h1>Acidity:</h1>
	<h2>Score: <%= cupping_acidity.score %></h2>
	<% if cupping_acidity.intensity.present? %>
		<h2>Intensity: <%= cupping_acidity.intensity %></h2>
	<% end %>
	<%= button_to 'Re-do', cupping_acidity, :method => :delete, :remote => true, :class => "button" %>
</section>