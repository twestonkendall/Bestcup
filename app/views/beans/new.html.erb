<% if @batch.blank? %>
	<header>
		<%= link_to "Back", farm_path(@farm) %>
	</header>
<% end %>
<% if @farm.blank? %>
	<header>
		<%= link_to "Back", new_blend_path(:batch_id => @batch.id, :roast_id => @roast.id)   %>
	</header>
<% end %>
<% if @farm.present? && @batch.present? %>
	<header>
		<%= link_to "Back", new_farm_path(:batch_id => @batch.id, :roast_id => @roast.id) %>
	</header>
<% end %>
<%= render 'form' %>